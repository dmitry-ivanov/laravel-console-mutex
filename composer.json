{
    "name": "illuminated/console-mutex",
    "description": "Prevents overlapping for Laravel console commands.",
    "keywords": ["laravel", "console", "command", "mutex", "overlapping", "locker"],
    "license": "MIT",
    "authors": [
        {
            "name": "Dmitry Ivanov",
            "email": "dmitry.g.ivanov@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/support": "5.1.*|5.2.*|5.3.*|5.4.*",
        "illuminate/console": "5.1.*|5.2.*|5.3.*|5.4.*",
        "arvenil/ninja-mutex": "^0.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0",
        "mockery/mockery": "^0.9",
        "orchestra/testbench": "^3.1",
        "illuminated/testing-tools": "^0.6",
        "predis/predis": "^1.1"
    },
    "suggest" : {
        "predis/predis": "If you want to use `redis` strategy."
    },
    "autoload": {
        "psr-4": {
            "Illuminated\\Console\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/ConsoleMutex/TestCase.php",
            "tests/ConsoleMutex/fixture/app"
        ]
    }
}
