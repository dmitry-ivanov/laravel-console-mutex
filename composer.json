{
    "name": "illuminated/console-mutex",
    "description": "Mutex for Laravel console commands.",
    "keywords": ["laravel", "console", "command", "mutex", "overlapping", "locker"],
    "license": "MIT",
    "authors": [
        {
            "name": "Dmitry Ivanov",
            "email": "dmitry.g.ivanov@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "illuminate/support": "5.7.*",
        "illuminate/console": "5.7.*",
        "arvenil/ninja-mutex": "^0.6",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "3.7.*",
        "illuminated/testing-tools": "5.7.*",
        "predis/predis": "^1.1.1",
        "ext-pdo_mysql": "*",
        "ext-redis": "*"
    },
    "autoload": {
        "psr-4": {
            "Illuminated\\Console\\Mutex\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/ConsoleMutex/TestCase.php",
            "tests/ConsoleMutex/fixture/app"
        ]
    }
}
