{
    "name": "illuminated/console-mutex",
    "description": "Mutex for Laravel console commands.",
    "keywords": ["laravel", "console", "command", "mutex", "overlapping", "locker"],
    "license": "MIT",
    "authors": [{
        "name": "Dmitry Ivanov",
        "email": "dmitry.g.ivanov@gmail.com"
    }],
    "require": {
        "php": "^7.1.3",
        "illuminate/support": "5.8.*|^6.0",
        "illuminate/console": "5.8.*|^6.0",
        "arvenil/ninja-mutex": "^0.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5|^8.0",
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "3.8.*",
        "illuminated/testing-tools": "5.8.*",
        "predis/predis": "^1.1.1",
        "ext-pdo_mysql": "*",
        "ext-redis": "*"
    },
    "autoload": {
        "psr-4": {
            "Illuminated\\Console\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/ConsoleMutex/TestCase.php",
            "tests/ConsoleMutex/fixture/app"
        ]
    }
}
